#include<iostream>
using namespace std;

int main() {
    int a[1000];
    int average;
    int n ;
    int sum = 0;
    cin >> n;
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    for (int i = 0; i < n; i++) {
        sum += a[i];
    }
    average = sum / n;
    cout << average;


    

    return 0;
}
