#include<iostream>

using namespace std;

int main() {
    int n;
    cin >> n;
    int sum = 0;
    int average;
    int arr[1000];
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    for (int i = 0; i < n; i++) {
        sum += arr[i];
    }
    average = sum / n;
    cout << average;
   

    return 0;
}
